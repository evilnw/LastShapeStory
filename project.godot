; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[android]

modules="org/godotengine/godot/GodotAdMob"

[application]

config/name="Last Shape Story"
run/main_scene="res://scenes/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

Settings="*res://scripts/Settings.cs"
Admanager="*res://scripts/admanager.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="expand"

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="player"
2d_physics/layer_2="enemy"
2d_physics/layer_3="bonus"
2d_physics/layer_4="wall"
2d_physics/layer_5="block"
2d_physics/layer_6="block_without_enemy"

[logging]

file_logging/log_path="res://logs/log.txt"

[node]

name_casing=2

[rendering]

environment/default_environment="res://default_env.tres"
